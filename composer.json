{
    "name": "brunocfalcao/crypto-crawler",
    "description": "Nidavellir crypto crawler",
    "keywords": ["Crypto"],
    "homepage": "https://github.com/brunocfalcao/crypto-crawler",
    "license": "MIT",
    "authors": [{
        "name": "Bruno Falcao",
        "email": "bruno.c.falcao@brunofalcao.me",
        "homepage": "https://www.brunocfalcao.com",
        "role": "Developer"
    }],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0",
        "brunocfalcao/crypto-commands": "dev-master",
        "brunocfalcao/crypto-cube": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Nidavellir\\CryptoCrawler\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nidavellir\\CryptoCrawler\\CryptoCrawlerServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
